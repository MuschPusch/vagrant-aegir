<%
  archive_url = 'http://us.archive.ubuntu.com/ubuntu/'
-%>
deb <%= archive_url %> <%= release %> main restricted
deb-src <%= archive_url %> <%= release %> main restricted
deb <%= archive_url %> <%= release %>-updates main restricted
deb-src <%= archive_url %> <%= release %>-updates main restricted
deb <%= archive_url %> <%= release %> universe
deb-src <%= archive_url %> <%= release %> universe
deb <%= archive_url %> <%= release %>-updates universe
deb-src <%= archive_url %> <%= release %>-updates universe
deb <%= archive_url %> <%= release %> multiverse
deb-src <%= archive_url %> <%= release %> multiverse
deb <%= archive_url %> <%= release %>-updates multiverse
deb-src <%= archive_url %> <%= release %>-updates multiverse
deb <%= archive_url %> <%= release %>-security main restricted
deb-src <%= archive_url %> <%= release %>-security main restricted
deb <%= archive_url %> <%= release %>-security universe
deb-src <%= archive_url %> <%= release %>-security universe
deb <%= archive_url %> <%= release %>-security multiverse
deb-src <%= archive_url %> <%= release %>-security multiverse
